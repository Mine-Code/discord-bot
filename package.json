    "sharp": "^0.34.2",
    "typescript": "^5.1.3",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@types/mathjax": "^0.0.40",
    "prettier": "^3.5.3",
    "tsx": "^4.20.3",
    "vitest": "^3.2.3"
  },
  "pnpm": {
